<div class='error_messages'>
</div>

<%= form_for @project, :remote => true do |f| %>


  <div class="field">
    <%= f.label :title %>
    <br>
    <%= f.text_field :title %>
  </div>
  
  <div class="fields">
    <%= f.label :description %>
    <br>
    <%= f.text_area :description %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>